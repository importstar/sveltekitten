		"prettier-plugin-tailwindcss": "^0.7.1",
		"svelte": "^5.43.4",
		"svelte-check": "^4.3.3",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.28.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.2",
		"vite-plugin-devtools-json": "^1.0.0",
		"zod": "^4.1.12"
	},
